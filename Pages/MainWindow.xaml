<Window x:Class="dom_texniki.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dom_texniki"
        mc:Ignorable="d"
        Title="Reg" Height="450" Width="800" Icon="/Resources/logo.ico">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Top"
          Width="auto"
          Background="#FFEEEEEE">
        <Grid.RowDefinitions>
            <RowDefinition Height="37*"/>
            <RowDefinition Height="180*"/>
        </Grid.RowDefinitions>
        <Image Source="/Resources/logo.png" Margin="175,4,550,5" />
        <TextBlock Text="Сервисный центр Дом техники" Margin="303,18,219,19" FontFamily="Corbel" FontSize="20" FontWeight="Bold"></TextBlock>
        <Frame NavigationUIVisibility="Hidden" x:Name="FrmMain" Grid.Row="1" Height="361" VerticalAlignment="Center"/>
        <Button Content="Назад" HorizontalAlignment="Left" Margin="24,0,0,0" VerticalAlignment="Center" Click="Button_Click" Background="#FFB9B7B7" Height="22" Width="58" Foreground="Black" BorderBrush="#00707070" FontFamily="Corbel"/>
    </Grid>
</Window>
